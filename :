import { rest } from "./common";
import { Routes } from "discord-api-types/v10";
import {warningChannelId} from "./env";

const main = async () => {
  await rest.post(
    Routes.channelMessages(warningChannelId),
    {
       body: {
       content: "This channel is for warnings only. A private thread will be created to notify warning when you did something violating the rules."
    }
  )
}

main();
